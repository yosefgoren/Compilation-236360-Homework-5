void main(){
	int a = 5;
	byte c = 4 b;
	bool x = true;
	while( a < 10){		
		print("in first loop");
		printi(a);
		
		
		while(c < 6 b){
			print("in second loop");
			printi(c);
			
			if( c == 5 ){
				print("break second loop");
				printi(c);
				
				break;
			}			
			
			print("second loop didn't break");
			printi(c);
			
			c = c + 1 b;
			
		}
		
		if(not x or a>7 ){
			print("break first loop");
			printi(a);
			
			break;
		}
		
		print("first loop didn't break");
		printi(a);
		
		
		a = a+1;
		c = c - 1 b;
	}
	print("bye bye");
}