int factorial(int a)
{
	if(a==1 or a==0)
		return 1;
	return factorial(a-1)*a;
}

int fibo(int a)
{
	if(a==1 or a==0)
		return 1;
	return fibo(a-1)+fibo(a-2);
}
void main()
{
	print("factorial number 5,7,9:");
	printi(factorial(5));
	
	printi(factorial(7));
	
	printi(factorial(9));
	
	print("fibo number 5,6,9:");
	printi(fibo(4+1));
	
	printi(fibo(2*4-2));
	
	printi(fibo(9));
	

}
